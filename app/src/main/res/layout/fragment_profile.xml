<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".fragments.ProfileFragment"
        android:screenOrientation="portrait"
        android:background="#525252"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="60dp"

            tools:context=".fragments.ProfileFragment"
            android:background="@color/black"
            tools:ignore="MissingConstraints"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="1.0"
            app:layout_constraintHorizontal_bias="1.0">

        <TextView
                android:text="@string/questionAnswer"
                android:fontFamily="sans-serif-medium"
                android:textColor="#FFFFFF"
                android:textSize="15sp"
                android:gravity="left|top"
                android:layout_width="148dp"
                android:layout_height="24dp"
                android:id="@+id/textViewEnterLogin6"
                tools:ignore="HardcodedText,MissingConstraints,RtlHardcoded"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintVertical_bias="0.456"
                android:layout_marginTop="80dp"
                app:layout_constraintHorizontal_bias="0.498"/>
        <TextView
                android:text="@string/options"
                android:fontFamily="sans-serif-medium"
                android:textColor="#FFFFFF"
                android:textSize="15sp"
                android:gravity="left|top"
                android:layout_width="148dp"
                android:layout_height="24dp"
                android:id="@+id/textViewEnterLogin5"
                tools:ignore="HardcodedText,MissingConstraints,RtlHardcoded"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintVertical_bias="0.381"
                android:layout_marginTop="80dp"
                app:layout_constraintHorizontal_bias="0.498"/>
        <ImageView
                android:src="@drawable/question"
                android:layout_width="24sp"
                android:layout_height="24sp"
                android:id="@+id/imageView7"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                tools:ignore="ContentDescription,MissingConstraints"
                app:layout_constraintHorizontal_bias="0.214"
                app:layout_constraintVertical_bias="0.511"/>
        <TextView
                android:text="@string/termAndCondition"
                android:fontFamily="sans-serif-medium"
                android:gravity="center"
                android:textColor="#BEBEBE"
                android:textSize="15sp"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:id="@+id/textViewEnterLogin2"
                tools:ignore="HardcodedText,MissingConstraints" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintVertical_bias="0.739"
                app:layout_constraintHorizontal_bias="0.31"
                android:layout_marginTop="100dp"/>
        <TextView
                android:text="@string/privacyPolicy"
                android:fontFamily="sans-serif-medium"
                android:gravity="center"
                android:textColor="#BEBEBE"
                android:textSize="15sp"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:id="@+id/textViewEnterLogin3"
                tools:ignore="HardcodedText,MissingConstraints" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintVertical_bias="0.086"
                app:layout_constraintHorizontal_bias="0.348"
                app:layout_constraintTop_toBottomOf="@+id/textViewEnterLogin2"/>
        <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/profile_image"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/profile"
                app:civ_border_width="3dp"
                app:civ_border_color="#FF000000"
                tools:ignore="MissingConstraints"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="0.498"
                app:layout_constraintVertical_bias="0.082"/>
        <TextView
                android:text=""
                android:layout_marginTop="70dp"
                android:fontFamily="sans-serif-medium"
                android:gravity="center"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:id="@+id/textViewEnterLogin"
                tools:ignore="HardcodedText,MissingConstraints" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintVertical_bias="0.165"
                app:layout_constraintHorizontal_bias="0.511"/>
        <ImageView
                android:src="@drawable/twitter"
                android:layout_width="24dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_height="24dp" android:id="@+id/imageView4"
                tools:ignore="ContentDescription,HardcodedText,MissingConstraints"
                app:layout_constraintHorizontal_bias="0.498" app:layout_constraintVertical_bias="0.669"/>
        <ImageView
                android:src="@drawable/instagram"
                android:layout_width="24dp"
                android:layout_height="24dp" android:id="@+id/imageView5"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="92dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                tools:ignore="ContentDescription,HardcodedText,MissingConstraints"
                app:layout_constraintHorizontal_bias="0.427" app:layout_constraintVertical_bias="0.669"/>
        <ImageView
                android:src="@drawable/facebook"
                android:layout_width="24dp"
                android:layout_height="24dp" android:id="@+id/imageView3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="92dp"
                tools:ignore="ContentDescription,HardcodedText,MissingConstraints"
                app:layout_constraintHorizontal_bias="0.569" app:layout_constraintVertical_bias="0.669"/>
        <ImageView
                android:src="@drawable/gear"
                android:layout_width="24sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_height="24sp" android:id="@+id/imageView6"
                tools:ignore="ContentDescription,HardcodedText,MissingConstraints"
                app:layout_constraintHorizontal_bias="0.214"
                app:layout_constraintVertical_bias="0.442"
        />

    </androidx.constraintlayout.widget.ConstraintLayout>
    <TextView
            android:text="@string/edit"
            android:fontFamily="sans-serif-medium"
            android:gravity="center"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            android:autoSizeTextType="uniform"
            android:layout_width="187dp"
            android:layout_height="26dp" android:id="@+id/textViewEnterLogin4"
            tools:ignore="HardcodedText,MissingConstraints"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="1.0" app:layout_constraintVertical_bias="0.019"/>
</androidx.constraintlayout.widget.ConstraintLayout>

